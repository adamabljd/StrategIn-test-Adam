<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>Users</title>
    </head>
    <body>
        <h1>List of Users</h1>

        <table>
            <thead>
            <tr>
                <th>Email</th>
                <th>Password</th>
                <th>Created At</th>
            </tr>
            </thead>
            <tbody>
            <% users.forEach((user) => { %>
                <tr>
                    <td><%= user.email %></td>
                    <td><%= user.password %></td>
                    <td><%= user.createdAt %></td>
                </tr>
            <% }); %>
            </tbody>
        </table>
        <br/>
        <form action="/" method="get">
            <button type="submit">Index</button>
        </form>
    </body>
</html>